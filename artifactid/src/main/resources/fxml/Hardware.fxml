<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="92.0" prefWidth="557.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.HWIDController">

    <TextField fx:id="text" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="528.0" promptText="Type the HWID you want to approve...">
        <effect>
            <DropShadow />
        </effect>
    </TextField>
    <Button fx:id="approve" layoutX="14.0" layoutY="55.0" mnemonicParsing="false" onAction="#action" prefHeight="25.0" prefWidth="260.0" text="Approve">
        <font>
            <Font name="System Bold Italic" size="12.0" />
        </font>
        <effect>
            <DropShadow />
        </effect>
    </Button>
    <Button fx:id="see" layoutX="282.0" layoutY="55.0" mnemonicParsing="false" onAction="#action2" prefHeight="25.0" prefWidth="260.0" text="See this PC's HWID">
        <font>
            <Font name="System Bold Italic" size="12.0" />
        </font>
        <effect>
            <DropShadow />
        </effect>
    </Button>

</AnchorPane>
